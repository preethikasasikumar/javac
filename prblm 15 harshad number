import java.util.Scanner;
class HarshadNumber {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int num = scanner.nextInt(), sum = 0, temp = num;
while (temp > 0) {
	sum += temp % 10;
        temp /= 10;
}
        System.out.println(num % sum == 0);
        scanner.close();
}
}
