import java.util.Scanner;
class MagicBoard {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the digits:");
        for (int i = 0; i < 4; i++) {
            int code = sc.nextInt();
            System.out.println(code + "-" + (char)code);
        }
    }
}
